<Window x:Class="laba9.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:laba9"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Menu>
            <MenuItem Header="Данные">
                <MenuItem Header="Поставщики">
                    <MenuItem Click="SuppliersTable_Click" Header="Общие сведения"/>
                    <MenuItem Click="PhysicalPersonsTable_Click" Header="Физические лица"/>
                    <MenuItem Click="LegalPersonsTable_Click" Header="Юридические лица"/>
                </MenuItem>
                <MenuItem Header="Поставки Товаров"/>
            </MenuItem>
            <MenuItem Header="Отчеты"/>
            <MenuItem Header="Выход" Click="Close_Click"/>
        </Menu>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainFrane" Margin="0,20,0,0"/>
    </Grid>
</Window>
